<?xml version="1.0" encoding="UTF-8"?>
<project name="module_x950" default="compile.module.x950">
  <dirname property="module.x950.basedir" file="${ant.file.module_x950}"/>
  
  <property name="module.jdk.home.x950" value="${jdk.home.android_api_21_platform}"/>
  <property name="module.jdk.bin.x950" value="${jdk.bin.android_api_21_platform}"/>
  <property name="module.jdk.classpath.x950" value="jdk.classpath.android_api_21_platform"/>
  
  <property name="compiler.args.x950" value="-encoding UTF-8 -source 1.6 ${compiler.args}"/>
  
  <property name="x950.output.dir" value="${module.x950.basedir}/../../../../Dropbox/0 Code/AndroidStudio/x950/out/production/x950"/>
  <property name="x950.testoutput.dir" value="${module.x950.basedir}/../../../../Dropbox/0 Code/AndroidStudio/x950/out/test/x950"/>
  
  <path id="x950.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="x950.module.production.classpath">
    <path refid="${module.jdk.classpath.x950}"/>
    <pathelement location="${drag-sort-listview.output.dir}"/>
    <pathelement location="${basedir}/../../../../Dropbox/0 Code/AndroidStudio/x950/libraries/dropbox-sync-sdk-android.jar"/>
    <pathelement location="${basedir}/../../../../Dropbox/0 Code/AndroidStudio/x950/libraries/systembartint-1.0.1.jar"/>
    <path refid="library.android-support-v4.classpath"/>
  </path>
  
  <path id="x950.runtime.production.module.classpath">
    <pathelement location="${x950.output.dir}"/>
    <path refid="drag-sort-listview.runtime.production.module.classpath"/>
    <pathelement location="${basedir}/../../../../Dropbox/0 Code/AndroidStudio/x950/libraries/dropbox-sync-sdk-android.jar"/>
    <pathelement location="${basedir}/../../../../Dropbox/0 Code/AndroidStudio/x950/libraries/systembartint-1.0.1.jar"/>
    <path refid="library.android-support-v4.classpath"/>
  </path>
  
  <path id="x950.module.classpath">
    <path refid="${module.jdk.classpath.x950}"/>
    <pathelement location="${x950.output.dir}"/>
    <pathelement location="${drag-sort-listview.testoutput.dir}"/>
    <pathelement location="${drag-sort-listview.output.dir}"/>
    <pathelement location="${basedir}/../../../../Dropbox/0 Code/AndroidStudio/x950/libraries/dropbox-sync-sdk-android.jar"/>
    <pathelement location="${basedir}/../../../../Dropbox/0 Code/AndroidStudio/x950/libraries/systembartint-1.0.1.jar"/>
    <path refid="library.android-support-v4.classpath"/>
  </path>
  
  <path id="x950.runtime.module.classpath">
    <pathelement location="${x950.testoutput.dir}"/>
    <pathelement location="${x950.output.dir}"/>
    <path refid="drag-sort-listview.runtime.module.classpath"/>
    <pathelement location="${basedir}/../../../../Dropbox/0 Code/AndroidStudio/x950/libraries/dropbox-sync-sdk-android.jar"/>
    <pathelement location="${basedir}/../../../../Dropbox/0 Code/AndroidStudio/x950/libraries/systembartint-1.0.1.jar"/>
    <path refid="library.android-support-v4.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.x950">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.x950">
    <patternset refid="excluded.from.module.x950"/>
  </patternset>
  
  <path id="x950.module.sourcepath">
    <dirset dir="${module.x950.basedir}/../../../../Dropbox/0 Code/AndroidStudio/x950">
      <include name="gen"/>
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.x950" depends="compile.module.x950.production,compile.module.x950.tests" description="Compile module x950"/>
  
  <target name="compile.module.x950.production" depends="register.custom.compilers,compile.module.drag-sort-listview" description="Compile module x950; production classes">
    <mkdir dir="${x950.output.dir}"/>
    <javac2 destdir="${x950.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.x950}/javac">
      <compilerarg line="${compiler.args.x950}"/>
      <bootclasspath refid="x950.module.bootclasspath"/>
      <classpath refid="x950.module.production.classpath"/>
      <src refid="x950.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.x950"/>
    </javac2>
    
    <copy todir="${x950.output.dir}">
      <fileset dir="${module.x950.basedir}/../../../../Dropbox/0 Code/AndroidStudio/x950/gen">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.x950.basedir}/../../../../Dropbox/0 Code/AndroidStudio/x950/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.x950.tests" depends="register.custom.compilers,compile.module.x950.production" description="compile module x950; test classes" unless="skip.tests"/>
  
  <target name="clean.module.x950" description="cleanup module">
    <delete dir="${x950.output.dir}"/>
    <delete dir="${x950.testoutput.dir}"/>
  </target>
</project>